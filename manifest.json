{
  "name": "Islamic Face Detector",
  "version": "2.0",
  "permissions": ["tabs", "<all_urls>"],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["scripts/face-api.min.js", "scripts/main.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": ["models/*", "wasm/*"]
}
